    },
    {
      "id": 1,
      "type": "stat",
      "title": "Redis 업타임 (초)",
      "datasource": "Prometheus",
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "colorMode": "none",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "horizontal",
        "showPercentChange": false,
        "textMode": "value"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "max(redis_uptime_in_seconds{instance=~\"$instance\"})"
        }
      ]
    },
    {
      "id": 2,
      "type": "stat",
      "title": "클라이언트 수",
      "datasource": "Prometheus",
      "gridPos": { "h": 6, "w": 6, "x": 6, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 100 },
              { "color": "red", "value": 200 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "horizontal",
        "showPercentChange": false,
        "textMode": "value"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(redis_connected_clients{instance=~\"$instance\"})"
        }
      ]
    },
